@using FEP.Model.eLearning
@model FEP.WebApiModel.eLearning.CreateOrEditContentModel

@Html.Partial("_contentCompletionDetail", Model)

<div>
    <div class="form-group">
        <label>@Language.eLearning.Content.IFrame</label>
        <div class="">

            @Html.EditorFor(model => model.Url, new { htmlAttributes = new { @class = "form-control", @readonly = "readonly" } })
        </div>
    </div>

    <div class="form-group">
        <label>@Language.eLearning.Content.ShowIFrameAs</label>
        <div class="">

            @Html.EnumDropDownListFor(model => model.ShowIFrameAs, htmlAttributes: new { @class = "form-control", @readonly = "readonly" })
        </div>
    </div>
</div>