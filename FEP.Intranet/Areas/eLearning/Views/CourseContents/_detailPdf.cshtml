@using FEP.Model.eLearning
@model FEP.WebApiModel.eLearning.CreateOrEditContentModel

@Html.Partial("_contentCompletionDetail", Model)


<div class="form-group">
    <div>
        <object width="100%" height="500">
            <embed src="~/eLearning/File/GetImg?fileName=@Model.FileDocument.FileNameOnStorage"
                   type="application/pdf" width="100%" height="500">
            </embed>
        </object>
    </div>
</div>

<div class="form-group">
    <label class="">Pdf document</label>
    <div>
        <object width="100%" height="600px" data="~/eLearning/File/GetPdf?fileName=@Model.FileDocument.FileNameOnStorage"></object>
    </div>

</div>


