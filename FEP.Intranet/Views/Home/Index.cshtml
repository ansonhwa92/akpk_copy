}

<section id="mainbanner">
    <!-- Start banner slider -->
    <div id="mainbannerslider" class="carousel slide carousel-fade" data-ride="carousel">
        <div class="carousel-inner">
            @if (Model.CarouselList.Count() > 0)
            {
                var count = 0;
                foreach (var carousel in Model.CarouselList)
                {
                    count++;
                    var position = carousel.TextLocation == FreeTextLocation.Left ? "text-left" : "text-center";
                    <div class="carousel-item @Html.Raw( (count == 1)? "active" : "")">
                        <img class="d-block w-100" src="@Url.Content("~/Data/images/carousel/" + carousel.CarouselImage)" alt="third slide">
                        <div class="carousel-caption @position intro-container d-none d-md-block wow fadeInUp">
                            @Html.Raw(carousel.FreeTextArea)
                        </div>
                    </div>
                }
            }
            else
            {
                //default carousel to avoid UI break
                <div class="carousel-item active">
                    <img class="d-block w-100" src="@Url.Content("~/img/banner/akpk-bnr-01.jpg")" alt="First slide">
                    <div class="carousel-caption text-center intro-container d-none d-md-block pt-5 wow fadeInUp">
                        <h1>Your financial well-being is <span class="color-primary">our concern</span></h1>
                        <p>We're a nonprofit with the mission to provide a free, world class education for anyone, anywhere.</p>
                        <div class="mt-4">
                            <a class="btn btn-outline-secondary mr-2" href="#">Tell Me More</a>
                            <a class="btn btn-link" href="#"><i class="la la-play-circle font-icon"></i> Watch Video</a>
                        </div>
                    </div>
                </div>
            }
        </div>

        <a class="carousel-control-prev" href="#mainbannerslider" role="button" data-slide="prev">
            <span class="carousel-control-prev-icon" aria-hidden="true"></span>
            <span class="sr-only">Previous</span>
        </a>
        <a class="carousel-control-next" href="#mainbannerslider" role="button" data-slide="next">
            <span class="carousel-control-next-icon" aria-hidden="true"></span>
            <span class="sr-only">Next</span>
        </a>
    </div>
</section>
