@model FEP.WebApiModel.Home.LandingPageModel
@{
    ViewBag.Title = "Home";
    Layout = "~/Views/Shared/_LayoutLandingPage.cshtml";
}


@section Preloader{

    <div id="preloader">
        <div class="loader"></div>
